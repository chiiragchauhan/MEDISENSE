{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "server.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
